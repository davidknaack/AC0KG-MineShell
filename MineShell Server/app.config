<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- Path to java.exe, in case it is not on the enviroment path. 
         Blank to rely on Windows finding it automatically.-->
    <add key="Java.exe Path" value="C:\Program Files (x86)\Java\jre7\bin"/>
    
    <!-- Parameters for java command. Use to set memory limits, server jar name, etc .
         Note that the '-nojline' arg is required for bukkit-based servers to get
         them to read from the standard input stream, which is required to send
         commands to the console.-->
    <add key="Java Params" value="-Xmx1024M -Xms1024M -jar tekkit.jar -nojline nogui"/>
    
    <!-- Includ messages from the minecraft console to the log. If running on the desktop
         instead of as a service, messages will appear on the console.
         "T" to enable, anything else to disable. -->
    <add key="Log console messages" value="F"/>
    
    <!-- Output directory for the MineShell log file. Blank for current directory. -->
    <add key="Log Dir" value=""/>
    
    <!-- Name of the script file to use for extended processing of server output. -->
    <add key="Script File" value="BaseScript.cs"/>
    
    <!-- TCP port for telnet access to remote terminal -->
    <add key="Remote Console Port" value="10023"/>
    
    <!-- Users allowed remote access. Comma-separated list.-->
    <add key="Remote Users" value="tom,dick,harry"/>

    <!-- User passwords. Comma-separated list, parallel to users.-->
    <add key="Remote User Passwords" value="123,456,789"/>
  </appSettings>
</configuration>
